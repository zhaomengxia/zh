<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zh.mapper.log.OperateLogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zh.entity.log.OperateLog">
        <id column="id" property="id"/>
        <result column="operator" property="operator"/>
        <result column="operate_role" property="operateRole"/>
        <result column="operate_time" property="operateTime"/>
        <result column="operator_ip" property="operatorIp"/>
        <result column="operate_active" property="operateActive"/>
        <result column="execute_time" property="executeTime"/>
        <result column="is_deleted" property="deleted"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        operator,
        operate_role,
        operate_time,
        operator_ip,
        operate_active,
        execute_time
        is_deleted,
        create_time,
        update_time
    </sql>

</mapper>
